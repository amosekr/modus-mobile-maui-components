<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:local="clr-namespace:Trimble.Modus.Components;assembly=Trimble.Modus.Components"
                    x:Class="Trimble.Modus.Components.BaseProgressBarStyles">

  <Style TargetType="local:BaseProgressBar" x:Key="PrimaryProgressBar">
    <Setter Property="ProgressColor" Value="{DynamicResource ProgressBarPrimaryProgressColor}" />
    <Setter Property="BaseColor" Value="{DynamicResource ProgressBarPrimaryBaseColor}" />
  </Style>

</ResourceDictionary>

